      "cell_type": "code",
      "source": [
        "model = {\n",
        "    \"env_id\": env_id,\n",
        "    \"max_steps\": max_steps,\n",
        "    \"n_training_episodes\": n_training_episodes,\n",
        "    \"n_eval_episodes\": n_eval_episodes,\n",
        "    \"eval_seed\": eval_seed,\n",
        "\n",
        "    \"learning_rate\": learning_rate,\n",
        "    \"gamma\": discount_factor,              #creating a dict to store model parameters\n",
        "    \"max_epsilon\": max_epsilon,\n",
        "    \"min_epsilon\": min_epsilon,\n",
        "    \"decay_rate\": decay_rate,\n",
        "\n",
        "    \"qtable\": Qtable_taxi\n",
        "}"
      ],
      "metadata": {
        "id": "f6LAN3wb_jzS"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def train(n_training_episodes,learning_rate,discount_factor,max_epsilon,min_epsilon,decay_rate,max_steps,env):\n",
        "        for episode in range(n_training_episodes):\n",
        "            state=env.reset()\n",
        "            state=state[0]\n",
        "            epsilon=min_epsilon+(max_epsilon-min_epsilon)*(np.exp(-decay_rate*episode))\n",
        "            step=0\n",
        "            terminated=False\n",
        "            truncated=False\n",
        "            reward=0\n",
        "\n",
        "            for step in range(max_steps):\n",
        "                if(np.random.rand()<epsilon):\n",
        "                    action=int(np.random.uniform(0,6))\n",
        "\n",
        "                else:\n",
        "                    action=np.argmax(Qtable_taxi[state])\n",
        "\n",
        "                info=env.step(action)\n",
        "                next_state=info[0]\n",
        "                reward=info[1]\n",
        "                terminated=info[2]\n",
        "                truncated=info[3]\n",
        "\n",
        "\n",
        "                Qtable_taxi[state,action] = Qtable_taxi[state,action] +learning_rate*(discount_factor*(np.max(Qtable_taxi[next_state])) +reward - Qtable_taxi[state,action])\n",
        "\n",
        "                if terminated or truncated:\n",
        "                    break\n",
        "                state=next_state\n",
        "                step=step+1\n",
        "\n",
        "        return Qtable_taxi"
      ],
      "metadata": {
        "id": "iKWVUq5x_5a3"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "Qtable_taxi = train(n_training_episodes,learning_rate,discount_factor,max_epsilon,min_epsilon,decay_rate,max_steps,env)\n"
      ],
      "metadata": {
        "id": "_pX6XYix_-ih"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def evaluate_agent(env, max_steps, n_eval_episodes, Qtable_taxi, seed):\n",
        "\n",
        "  episode_rewards = []\n",
        "  for episode in range(n_eval_episodes):\n",
        "    if seed:\n",
        "      state= env.reset(seed=seed[episode])\n",
        "    else:\n",
        "      state= env.reset()\n",
        "    state=state[0]\n",
        "\n",
        "    truncated = False\n",
        "    terminated = False\n",
        "    total_rewards_ep = 0\n",
        "    step=0\n",
        "\n",
        "    for step in range(max_steps):\n",
        "      # Take the action (index) that have the maximum expected future reward given that state\n",
        "      action = np.argmax(Qtable_taxi[state])\n",
        "\n",
        "      info=env.step(action)\n",
        "      next_state=info[0]\n",
        "      reward=info[1]\n",
        "      terminated=info[2]\n",
        "      truncated=info[3]\n",
        "      total_rewards_ep += reward\n",
        "\n",
        "      if terminated or truncated:\n",
        "        break\n",
        "      state = next_state\n",
        "    episode_rewards.append(total_rewards_ep)\n",
        "  mean_reward = np.mean(episode_rewards)\n",
        "  std_reward = np.std(episode_rewards)\n",
        "\n",
        "  return mean_reward, std_reward\n"
      ],
      "metadata": {
        "id": "Uh0rUFZRAFAk"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "mean_reward,stad_reward = evaluate_agent(env, max_steps, n_eval_episodes, Qtable_taxi, eval_seed)\n",
        "print(mean_reward,stad_reward)\n",
        "\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "oBKDxsz_AF7c",
        "outputId": "ff3201a9-36ca-4abc-e70a-55586a0fdc9c"
      },
      "execution_count": null,
      "outputs": [
